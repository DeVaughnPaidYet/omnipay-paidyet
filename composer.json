{
    "name": "omnipay/paidyet",
    "type": "library",
    "description": "PaidYET driver for the Omnipay payment processing library",
    "keywords": [
        "paidyet",
        "paid yet",
        "gateway",
        "merchant",
        "secure",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/devaughnpaidyet/ommnipay-paidyet",
    "license": "MIT",
    "authors": [
        {
            "name": "DeVaughn Skillern",
            "email": "devaughn@paidyet.com"
        },
        {
            "name": "PaidYET Contributors",
            "homepage": "https://github.com/devaughnpaidyet/ommnipay-paidyet/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\PaidYET\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3",
        "guzzlehttp/guzzle": "^7.8",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "squizlabs/php_codesniffer": "^3",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
